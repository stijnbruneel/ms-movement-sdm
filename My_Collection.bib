journal = {Journal of Marine Systems},
keywords = {Age,Exposure time,Finite-element,Model,Residence time,Scheldt Estuary,Water renewal timescales},
pages = {3--16},
publisher = {Elsevier B.V.},
title = {{Reprint of water renewal timescales in the Scheldt Estuary}},
url = {http://dx.doi.org/10.1016/j.jmarsys.2011.10.013},
volume = {128},
year = {2013}
