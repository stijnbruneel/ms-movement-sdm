file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Peterson, Nakazawa - 2008 - Environmental data sets matter in ecological niche modelling An example with Solenopsis invicta and Solenops.pdf:pdf},
isbn = {1466-822X$\backslash$n1466-8238},
issn = {1466822X},
journal = {Global Ecology and Biogeography},
keywords = {Climatic data,Data sources,Ecological niche modelling,Fire ant,Invasive species,Niche shifts},
number = {1},
pages = {135--144},
pmid = {6591},
title = {{Environmental data sets matter in ecological niche modelling: An example with Solenopsis invicta and Solenopsis richteri}},
volume = {17},
year = {2008}
}
@article{Fierer2005,
abstract = {The temperature sensitivity of litter decomposition will influence the rates of ecosystem carbon sequestration in a warmer world. A number of studies have shown that the temperature sensitivity of litter decomposition can vary depending on litter type and extent of decomposition. However, the underlying causes of this variation are not well understood. According to fundamental principles of enzyme kinetics, the temperature sensitivity of microbial decomposition should be inversely related to litter carbon quality. We tested the accuracy of this hypothesis by adding ground plant shoot and root material to soils incubated under controlled conditions and measuring the temperature sensitivities of decomposition at three time points throughout a 53-d incubation. As the overall quality of the litter organic C declined, litter decomposition became more sensitive to temperature. This was true regardless of whether differences in C quality were due to inherent differences in litter chemistry or due to differences ...},
