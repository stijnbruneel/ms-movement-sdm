isbn = {00220981},
issn = {00220981},
journal = {Journal of Experimental Marine Biology and Ecology},
keywords = {Acoustic telemetry,Benthic predator,Ecological function,Habitat restoration,Mustelus californicus,Seasonally thermal environment,Southern California estuary},
number = {1-2},
pages = {63--74},
publisher = {Elsevier B.V.},
title = {{Habitat use, movements and site fidelity of the gray smooth-hound shark (Mustelus californicus Gill 1863) in a newly restored southern California estuary}},
