file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/de Brye et al. - 2010 - A finite-element, multi-scale model of the Scheldt tributaries, river, estuary and ROFI(2).pdf:pdf},
isbn = {0378-3839},
issn = {03783839},
journal = {Coastal Engineering},
keywords = {Finite-element,Hydrodynamics,Lagrangian residual velocity,Model,Multi-scale,Salinity,Scheldt Estuary,Tide},
number = {9},
pages = {850--863},
publisher = {Elsevier B.V.},
title = {{A finite-element, multi-scale model of the Scheldt tributaries, river, estuary and ROFI}},
