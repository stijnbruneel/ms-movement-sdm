@article{Testa2008,
abstract = {We conducted a quantitative assessment of estuarine ecosystem responses to reduced phosphorus and nitrogen loading from sewage$\backslash$n treatment facilities and to variability in freshwater flow and nonpoint nutrient inputs to the Patuxent River estuary. We$\backslash$n analyzed a 19-year dataset of water quality conditions, nutrient loading, and climatic forcing for three estuarine regions$\backslash$n and also computed monthly rates of net production of dissolved O2 and physical transport of dissolved inorganic nitrogen (DIN) and phosphorus (DIP) using a salt- and water-balance model.$\backslash$n Point-source loading of DIN and DIP to the estuary declined by 40–60{\%} following upgrades to sewage treatment plants and correlated$\backslash$n with parallel decreases in DIN and DIP concentrations throughout the Patuxent. Reduced point-source nutrient loading and concentration$\backslash$n resulted in declines in phytoplankton chlorophyll-a (chl-a) and light-saturated carbon fixation, as well as in bottom-layer O2 consumption for upper regions of the estuary. Despite significant reductions in seaward N transport from the middle to lower$\backslash$n estuary, chl-a, turbidity, and surface-layer net O2 production increased in the lower estuary, especially during summer. This degradation of water quality in the lower estuary$\backslash$n appears to be linked to a trend of increasing net inputs of DIN into the estuary from Chesapeake Bay and to above-average$\backslash$n river flow during the mid-1990s. In addition, increased abundance of Mnemiopsis leidyi significantly reduced copepod abundance during summer from 1990 to 2002, which favored increases in chl-a and allowed a shift in total N partitioning from DIN to particulate organic nitrogen. These analyses illustrate (1) the value$\backslash$n of long-term monitoring data, (2) the need for regional scale nutrient management that includes integrated estuarine systems,$\backslash$n and (3) the potential water quality impacts of altered coastal food webs.},
author = {Testa, Jeremy and Kemp, W. and Boynton, Walter and Hagy, James},
doi = {10.1007/s12237-008-9095-y},
isbn = {1559-2723},
issn = {1559-2723},
journal = {Estuaries and Coasts},
keywords = {eutrophication,patuxent river,water quality},
number = {6},
pages = {1021--1037},
title = {{Long-Term Changes in Water Quality and Productivity in the Patuxent River Estuary: 1985 to 2003}},
volume = {31},
year = {2008}
}
