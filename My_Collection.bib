@article{Still1999,
abstract = {Tropical montane cloud forests are unique among terrestrial ecosystems in that they are strongly linked to regular cycles of cloud formation. We have explored changes in atmospheric parameters from global climate model simulations of the Last Glacial Maximum and for doubled atmospheric carbon dioxide concentration (2 x CO2) conditions which are associated with the height of this cloud formation, and hence the occurrence of intact cloud forests. These parameters include vertical profiles of absolute and relative humidity surfaces, as well as the warmth index(1), an empirical proxy of forest type. For the glacial simulations, the warmth index and absolute humidity suggest a downslope shift of cloud forests that agrees with the available palaeodata. For the 2 x CO2 scenario, the relative humidity surface is shifted upwards by hundreds of metres during the winter dry season when these forests typically rely most on the moisture from cloud contact. At the same time, an increase in the warmth index implies increased evapo-transpiration. This combination of reduced cloud contact and increased evapo-transpiration could have serious conservation implications, given that these ecosystems typically harbour a high proportion of endemic species and are often situated on mountain tops or ridge lines.},
author = {Still, C J and Foster, P N and Schneider, S H},
doi = {10.1038/19293},
isbn = {0028-0836},
issn = {0028-0836},
journal = {Nature},
keywords = {CO2,ICE CORES,LAST GLACIAL MAXIMUM,MODEL,SEA-SURFACE TEMPERATURE},
number = {6728},
pages = {608--610},
title = {{Simulating the effects of climate change on tropical montane cloud forests}},
volume = {398},
year = {1999}
}
@article{Fierer2011,
abstract = {The elevational gradient in plant and animal diversity is one of the most widely documented patterns in ecology and, although no consensus explanation exists, many hypotheses have been proposed over the past century to explain these patterns. Historically, research on elevational diversity gradients has focused almost exclusively on plant and animal taxa. As a result, we do not know whether microbes exhibit elevational gradients in diversity that parallel those observed for macroscopic taxa. This represents a key knowledge gap in ecology, especially given the ubiquity, abundance, and functional importance of microbes. Here we show that, across a montane elevational gradient in eastern Peru, bacteria living in three distinct habitats (organic soil, mineral soil, and leaf surfaces) exhibit no significant elevational gradient in diversity (r2{\textless}0.17, P{\textgreater}0.1 in all cases), in direct contrast to the significant diversity changes observed for plant and animal taxa across the same montane gradient (r2{\textgreater}0.75, P{\textless}0.001 in all cases). This finding suggests that the biogeographical patterns exhibited by bacteria are fundamentally different from those of plants and animals, highlighting the need for the development of more inclusive concepts and theories in biogeography to explain these disparities.},
author = {Fierer, Noah and Mccain, Christy M. and Meir, Patrick and Zimmermann, Michael and Rapp, Joshua M. and Silman, Miles R. and Knight, Rob},
doi = {10.1890/10-1170.1},
isbn = {0012-9658},
issn = {00129658},
journal = {Ecology},
keywords = {16S rRNA genes,Bacterial diversity,Montane diversity gradient,Phyllosphere bacteria,Pyrosequencing,Soil bacteria},
number = {4},
pages = {797--804},
pmid = {21661542},
title = {{Microbes do not follow the elevational diversity patterns of plants and animals}},
volume = {92},
year = {2011}
}
@misc{Ferry2012,
abstract = {1.A recent article published by Toledo et al. (2011b) investigates$\backslash$nthe effects of spatial variations in climate and soil, and of logging$\backslash$ndisturbance, on tree and forest growth in Bolivia. It concludes that$\backslash$nclimate is the strongest driver of tree and forest growth and that$\backslash$nclimate change may therefore have large consequences for forest productivity$\backslash$nand carbon sequestration. However, serious methodological and conceptual$\backslash$ndiscrepancies have been found that challenge these conclusions. 2.Because$\backslash$nof an errant coding of ‘time after logging' in the regression$\backslash$nanalysis, and because floristic changes induced by logging could$\backslash$nnot be incorporated into the analysis, the effect of logging on the$\backslash$naverage diameter growth is likely to have been strongly underestimated.$\backslash$n3.Basal area growth was improperly calculated as basal area change,$\backslash$nand it displayed surprisingly high values, even among unlogged plots.$\backslash$nWe hypothesize that either these plots may be actually located in$\backslash$nsecondary forests recovering from past logging, or measurement biases$\backslash$nmay have hampered the data set. 4.Regardless of climate–growth$\backslash$nrelationships established across these plots, any inference concerning$\backslash$nthe potential effects of climate change on forest growth would require$\backslash$na specific quantitative assessment. 5.Synthesis. It is critical to$\backslash$nre-assess the relative weight of climate and logging disturbance$\backslash$nas driving factors of tree and forest growth, and to find an explanation$\backslash$nfor the very high basal area increment reported among the unlogged$\backslash$nplots. We provide specific recommendations for further analyses of$\backslash$nthis and similar data sets.},
