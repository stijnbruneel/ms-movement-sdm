issn = {2041-1723},
journal = {Nature communications},
keywords = {Anguilla,Anguilla: physiology,Animal Migration,Animals,Female,Male,Oceans and Seas,Swimming},
language = {en},
month = {oct},
pages = {8705},
pmid = {26505325},
publisher = {Nature Publishing Group},
title = {{Direct observations of American eels migrating across the continental shelf to the Sargasso Sea.}},
url = {http://www.nature.com/ncomms/2015/151027/ncomms9705/full/ncomms9705.html},
volume = {6},
