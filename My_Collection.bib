doi = {10.5194/bgd-6-3993-2009},
file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Quesada et al. - 2009 - Regional and large-scale patterns in Amazon forest structure and function are mediated by variations in soil phy.pdf:pdf},
