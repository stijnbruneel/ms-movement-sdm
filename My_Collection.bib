issn = {18791026},
journal = {Science of the Total Environment},
keywords = {Facility location problem,Interval analysis,Municipal solid waste management,Optimization,Uncertainty},
number = {October 2016},
pages = {760--771},
pmid = {28395953},
publisher = {Elsevier B.V.},
title = {{A facility location model for municipal solid waste management system under uncertain environment}},
url = {https://doi.org/10.1016/j.scitotenv.2017.02.207},
volume = {603-604},
