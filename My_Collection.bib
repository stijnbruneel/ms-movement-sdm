title = {{A Comparison of Modelling Systems for the Development and Revision of Water Resources Management Plans}},
url = {http://apps.webofknowledge.com/full{\_}record.do?product=WOS{\&}search{\_}mode=GeneralSearch{\&}qid=1{\&}SID=P1oXZpIR2fbmftG6kli{\&}page=1{\&}doc=3},
