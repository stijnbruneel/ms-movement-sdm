pages = {20--32},
title = {{Interspecific and inter-site variation in wood specific gravity of tropical trees}},
url = {http://apps.isiknowledge.com.ezproxy.lib.monash.edu.au/InboundService.do?Func=Frame{\&}product=WOS{\&}action=retrieve{\&}SrcApp=Papers{\&}UT=000220383700003{\&}SID=1E8dDOp3nign6EDh628{\&}Init=Yes{\&}SrcAuth=mekentosj{\&}mode=FullRecord{\&}customersID=mekentosj{\&}DestFail=http{\%}253A{\%}25},
