isbn = {0036-8075},
issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Anguilla,Anguilla: physiology,Animal Migration,Animals,Atlantic Ocean,Body Temperature Regulation,Ecosystem,Europe,Reproduction,Swimming,Temperature,Water Movements},
number = {5948},
