@article{Block2016,
abstract = {Animal telemetry is the science of elucidating the movements and behavior of animals in relation to their $\backslash$r$\backslash$nenvironment or habitat. Here, we focus on telemetry of aquatic species (marine mammals, sharks, fish, sea birds and $\backslash$r$\backslash$nturtles) and so are concerned with animal movements and behavior as they move through and above the world's $\backslash$r$\backslash$noceans, coastal rivers, estuaries and great lakes. Animal telemetry devices (“tags”) yield detailed data regarding animal $\backslash$r$\backslash$nresponses to the coupled ocean–atmosphere and physical environment through which they are moving. Animal $\backslash$r$\backslash$ntelemetry has matured and we describe a developing US Animal Telemetry Network (ATN) observing system that $\backslash$r$\backslash$nmonitors aquatic life on a range of temporal and spatial scales that will yield both short- and long-term benefits, fill $\backslash$r$\backslash$noceanographic observing and knowledge gaps and advance many of the U.S. National Ocean Policy Priority Objec-$\backslash$r$\backslash$ntives. ATN has the potential to create a huge impact for the ocean observing activities undertaken by the U.S. Inte$\backslash$r$\backslash$n-$\backslash$r$\backslash$ngrated Ocean Observing System (IOOS) and become a model for establishing additional national-level telemetry $\backslash$r$\backslash$nnetworks worldwide.$\backslash$r$\backslash$nBackground:$\backslash$r$\backslash$nTelemetry can provide environmental, behavioral and physiological data in near-real time, or by use of $\backslash$r$\backslash$narchival tags in which the data are stored or later transmitted to satellites. Aquatic animal species tagged have ranged $\backslash$r$\backslash$nfrom 6-g salmon smolts to 150-ton whales. Detailed observations of animal movements and behavior in relation to $\backslash$r$\backslash$ncritical habitats in their aquatic environment have significantly improved our understanding of ecosystem function $\backslash$r$\backslash$nand dynamics. These observations are critical for sustaining populations, conserving biodiversity and implementing $\backslash$r$\backslash$necosystem-based management through an increased understanding of ecosystem structures, functions, and pro$\backslash$r$\backslash$n-$\backslash$r$\backslash$ncesses, as well as their importance to ecosystem services and values. Sensors carried by tagged animals have come $\backslash$r$\backslash$nof age and deliver high-resolution physical oceanographic data at relatively low costs. Animals are particularly adept $\backslash$r$\backslash$nat helping scientists identify critical habitats, spawning locations, and important oceanographic features (e.g., fronts, $\backslash$r$\backslash$neddies and upwelling areas). They also provide important insights into regions of the oceans that are difficult and $\backslash$r$\backslash$nexpensive to monitor (e.g., offshore environments, Arctic). This paper focuses on how to integrate an operational ATN $\backslash$r$\backslash$ninto U.S. IOOS.$\backslash$r$\backslash$nResults:$\backslash$r$\backslash$nThe development of U.S. IOOS initially focused on the acquisition and integration of physical and chemical $\backslash$r$\backslash$noceanographic data. With this system now operational, U.S. IOOS is ready to add the acquisition of relevant biological $\backslash$r$\backslash$nobservations, and to enhance the acquisition of physical and chemical oceanographic observations via ATN platforms.$\backslash$r$\backslash$nConclusion:$\backslash$r$\backslash$nA U.S. ATN observing system that monitors aquatic life on a range of temporal and spatial scales could $\backslash$r$\backslash$nyield both short- and long-term benefits, fill oceanographic observing and knowledge gaps, and advance many of $\backslash$r$\backslash$nthe National Ocean Policy Priority Objectives. ATN has the potential to create a huge impact for the ocean observing activities undertaken by IOOS and become a model for establishing additional national-level telemetry networks $\backslash$r$\backslash$nworldwide.},
author = {Block, Barbara A. and Holbrook, Christopher M. and Simmons, Samantha E. and Holland, Kim N. and Ault, Jerald S. and Costa, Daniel P. and Mate, Bruce R. and Seitz, Andrew C. and Arendt, Michael D. and Payne, John C. and Mahmoudi, Behzad and Moore, Peter and Price, James M. and Levenson, J. Jacob and Wilson, Doug and Kochevar, Randall E.},
doi = {10.1186/s40317-015-0092-1},
file = {:C$\backslash$:/Users/spbrunee/Desktop/Data/DOC/AJ 2017-2018/Artikels/s40317-015-0092-1.pdf:pdf},
isbn = {4031701500},
