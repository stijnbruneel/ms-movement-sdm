@article{Usseglio2016,
abstract = {The regionally endemic Galapagos Grouper, locally known as bacalao, is$\backslash$none of the most highly prized finfish species within the Galapagos$\backslash$nMarine Reserve (GMR). Concerns of overfishing, coupled with a lack of$\backslash$nfishing regulations aimed at this species raises concerns about the$\backslash$ncurrent population health. We assessed changes in population health over$\backslash$na 30-year period using three simple indicators: (1) percentage of fish$\backslash$nbelow reproductive size (Lm); (2) percentage of fish within the optimum$\backslash$nlength interval (Lopt); and (3) percentage of mega-spawners in the$\backslash$ncatch. Over the assessed period, none of the indicators reached values$\backslash$nassociated with healthy populations, with all indicators declining over$\backslash$ntime. Furthermore, the most recent landings data show that the vast$\backslash$nmajority of the bacalao caught (95.7{\%},) were below L-m, the number of$\backslash$nfish within the Lopt interval was extremely low (4.7{\%}), and there were$\backslash$nvirtually no mega-spawners (0.2{\%}). Bacalao fully recruit to the fishery$\backslash$n15 cm below the size at which 50{\%} of the population matures. The$\backslash$nSpawning Potential Ratio is currently 5{\%} of potential unfished$\backslash$nfecundity, strongly suggesting severe overfishing. Our results suggest$\backslash$nthe need for bacalao-specific management regulations that should include$\backslash$nminimum (65 cm TL) and maximum (78 cm TL) landing sizes, slot limits$\backslash$n(64-78 cm TL), as well as a closed season during spawning from October$\backslash$nto January. It is recognized that these regulations are harsh and will$\backslash$ncertainly have negative impacts on the livelihoods of fishers in the$\backslash$nshort term, however, continued inaction will likely result in a collapse$\backslash$nof this economically and culturally valuable species. Alternative$\backslash$nsources of income should be developed in parallel with the establishment$\backslash$nof fishing regulations to limit the socio-economic disruption to the$\backslash$nfishing community during the transition to a more sustainable management$\backslash$nregime.},
author = {Usseglio, Paolo and Friedlander, Alan M. and Koike, Haruko and Zimmerhackel, Johanna and Schuhbauer, Anna and Eddy, Tyler and Salinas-de-Le{\'{o}}n, Pelayo},
doi = {10.1371/journal.pone.0165167},
file = {:C$\backslash$:/Users/spbrunee/Desktop/Data/DOC/AJ 2017-2018/Beurzen/WWF/Literatuur/journal.pone.0165167.PDF:PDF},
isbn = {1932-6203},
issn = {19326203},
