isbn = {02727714},
issn = {02727714},
journal = {Estuarine, Coastal and Shelf Science},
keywords = {Estuary,Guadalquivir river,Gulf of Cadiz,Harmonic analysis,Radiational tide,Temperature oscillations},
pages = {60--66},
publisher = {Elsevier Ltd},
title = {{About the tidal oscillations of temperature in a tidally driven estuary: The case of Guadalquivir estuary, southwest Spain}},
url = {http://dx.doi.org/10.1016/j.ecss.2012.06.007},
volume = {111},
