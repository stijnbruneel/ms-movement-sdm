isbn = {0272-7714},
issn = {02727714},
journal = {Estuarine, Coastal and Shelf Science},
keywords = {Biological traits,Conservation,Escapement,Europe,European Eel Management Plan,France,Loire estuary,Silver eel,Spawning migration},
number = {1},
pages = {23--31},
title = {{Migration behaviour of silver eels (Anguilla anguilla) in a large estuary of Western Europe inferred from acoustic telemetry}},
