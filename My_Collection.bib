@article{Pauwels2014,
abstract = {Northern pike, Esox lucius, needs different habitats to survive and$\backslash$nreproduce and thus depends on the availability and accessibility of$\backslash$nthese habitats. To efficiently manage pike, information is needed on its$\backslash$nspatial and temporal patterns of migration. In this study, we$\backslash$ninvestigated the occurrence of adult pike migration and which$\backslash$nenvironmental variables influenced migration. From December 2010, we$\backslash$nfollowed 15 pike for 1 year by use of radio telemetry in the River Yser,$\backslash$na typical lowland river characterised by anthropogenic impacts such as$\backslash$nartificial embankments. Pike migrated most in February and March, which$\backslash$ncould indicate they frequented spawning habitat in this period. Four$\backslash$nenvironmental variables significantly affected pike migration, ranging$\backslash$nfrom the location where pike were observed (strongest effect), over$\backslash$nwater temperature and flow to diel water temperature change (weakest$\backslash$neffect). The relation between migration and the location where pike were$\backslash$nobserved could demonstrate that pike preferred specific regions in the$\backslash$nriver. Increasing water temperature triggered migration for both sexes,$\backslash$nand males started migrating at lower temperatures than females, which$\backslash$nsuggests that males start migrating earlier. This was the only$\backslash$nsubstantial difference observed between male and female pike migration.$\backslash$nThe results suggest that migration was inhibited by high flow, as no$\backslash$nmigration was observed at high flow. River managers can use this$\backslash$ninformation to efficiently manage their pike populations, for example,$\backslash$nby removing or temporarily opening hydraulic structures like valves,$\backslash$nweirs and sluices. This may facilitate access to suitable habitats at$\backslash$nmoments pike needs these habitats to fulfil its life cycle.},
author = {Pauwels, Ine S. and Goethals, Peter L M and Coeck, Johan and Mouton, Ans M.},
doi = {10.1111/eff.12090},
file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pauwels et al. - 2014 - Movement patterns of adult pike (Esox lucius L.) in a Belgian lowland river.pdf:pdf},
issn = {16000633},
journal = {Ecology of Freshwater Fish},
keywords = {Fisheries management,Northern pike (Esox lucius),Patterns of movement,Radio telemetry,River management},
number = {3},
pages = {373--382},
title = {{Movement patterns of adult pike (Esox lucius L.) in a Belgian lowland river}},
volume = {23},
year = {2014}
}
