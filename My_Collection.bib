file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Baldigo et al. - 2017 - Efficacy of Environmental DNA to Detect and Quantify Brook Trout Populations in Headwater Streams of the Adirond.pdf:pdf},
