journal = {Biogeosciences},
number = {5},
pages = {1081--1106},
title = {{Height-diameter allometry of tropical forest trees}},
volume = {8},
year = {2011}
}
@misc{Merckx2010,
abstract = {In order to protect the biodiversity of the seas from, e.g.,$\backslash$noverexploitation, the spatial distribution of biodiversity and the$\backslash$nmapping of biodiversity hotspots are of great importance. In the present$\backslash$npaper we discuss different methods to develop full coverage biodiversity$\backslash$nmaps of free-living marine nematodes in the Southern Bight of the North$\backslash$nSea. A database with sampling data, gathered over 3 decades (1972 to$\backslash$n2004), combined with exhaustive environmental data, was employed to$\backslash$npredict species richness and the expected number of species by different$\backslash$nmethods: ordinary kriging (OK) and regression kriging (RK) with ordinary$\backslash$nleast squares (OLS) and generalised least squares (GLS). The predictive$\backslash$nvalue of these methods was evaluated by an independent validation set.$\backslash$nReplicate samples were used to make an accurate estimation of the nugget$\backslash$nvariance, since replicates reveal local variability. Accordingly, it was$\backslash$nfeasible to find a spatial pattern in the residuals of the regression$\backslash$nmodels. Our analysis pointed out that GLS improved the OK models$\backslash$nsubstantially, while RK only slightly improved the GLS model. The$\backslash$ndiversity of marine nematodes is substantially influenced by the$\backslash$nsilt-clay fraction and the amount of total suspended matter, which is$\backslash$nalso reflected in the resulting map with a species-poor area near the$\backslash$ncoast line, especially near the south of the mouth of the Scheldt$\backslash$nestuary. Off coast diversity and evenness are generally higher.},
