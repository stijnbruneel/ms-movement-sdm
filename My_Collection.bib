isbn = {0304-3800},
issn = {03043800},
journal = {Ecological Modelling},
keywords = {Community detection,Coral,Graph theory,Great Barrier Reef,Larval dispersal,Marine connectivity},
pages = {160--174},
publisher = {Elsevier B.V.},
title = {{Numerical modelling and graph theory tools to study ecological connectivity in the Great Barrier Reef}},
url = {http://dx.doi.org/10.1016/j.ecolmodel.2013.10.002},
volume = {272},
