@article{MoseraGeraldChristophLeuschneraMarinaRodersteinaSophieGraefea2010,
abstract = {Background: Data on below-ground production of tropical montane forests along elevation gradients are scarce. Aims: To determine fine, coarse and large root biomass and productivity along a 2000 m elevation transect. Methods: In five south Ecuadorian mountain forests along a transect from 1000 to 3000 m above sea level, fine ({\textless} 2 mm diameter), coarse (2–50 mm) and large root biomass ({\textgreater} 50 mm) were analysed by soil coring and excavation of soil pits. Fine root production was estimated synchronously by three different approaches (sequential soil coring, the ingrowth core method, and the mini-rhizotron technique). Coarse and large root production was estimated by recording diameter increment using dendrometer tapes. Results: Fine root biomass increased four-fold between 1000 and 3000 m; coarse and large root biomass doubled. The three approaches for estimating fine root production yielded highly divergent results, with the mini-rhizotron approach giving the most reliable data, and indicating a significant increase in fine root production with elevation. Conclusions: Our results indicate a marked carbon allocation shift from above- to below-ground towards higher elevations, which is probably a consequence of increasing nutrient limitation of tree growth with increasing elevation.},
author = {{Mosera, Gerald Christoph Leuschnera, Marina R{\"{o}}dersteina, Sophie Graefea}, Nathalie Soetheb and Dietrich Hertela},
doi = {10.1016/j.celrep.2011.1011.1001.7.},
isbn = {1111111111},
issn = {1755-0874},
journal = {Plant Ecology and Diversity},
keywords = {root biomass,root decomposition,root necromass,root production,soil chemistry,temperature,tropical rainforest},
number = {2},
pages = {151--164},
title = {{Biomass and productivity of fine and coarse roots in five tropical mountain forests stands along an altitudinal transect in southern Ecuador}},
url = {http://www.informaworld.com/index/713824049.pdf},
volume = {3},
year = {2010}
}
@article{Bijoux2013,
abstract = {The shoemaker spinefoot Siganus sutor is a fast-growing, short-lived,$\backslash$ngonochorist and herbivorous reef fish that forms transient fish spawning$\backslash$naggregations (FSAs). In the Seychelles, FSAs of this species occur$\backslash$nmonthly between September and June around the full moon. To study the$\backslash$nspawning aggregation dynamics of S. sutor, 39 reproductively active$\backslash$nindividuals were acoustically tagged and monitored at 3 known FSA sites$\backslash$n(Pate Polite, Pate Desire and Pate Dividi) off the west coast of Praslin$\backslash$nisland (Seychelles) between October 2010 and April 2011. The presence$\backslash$nand absence of tagged fish at the 3 sites were monitored using moored$\backslash$narrays of passive acoustic receivers, while the density of fish at the$\backslash$nFSAs was quantified using underwater visual census (UVC). Most tagged$\backslash$nfish (87.0{\%}) detected in more than 1 spawning period showed fidelity to$\backslash$nsingle spawning sites. Predominantly, tagged individuals arrived at FSA$\backslash$nsites at dawn and departed at dusk. Spawning residency times at FSA$\backslash$nsites ranged from 0.02 to 16.1 d. At Pate Polite, residency times were$\backslash$nhigher in the first half of the spawning season. Residency times were$\backslash$noften several-fold shorter than aggregation duration which, when$\backslash$ncombined with patterns in arrival and departure times, indicates high$\backslash$nturnover of fish within aggregations. The findings of this study provide$\backslash$ninformation needed for emerging co-management schemes that aim to$\backslash$nsustain this socio-economically important fishery.},
author = {Bijoux, Jude P. and Dagorn, Laurent and Berke, Gregory and Cowley, Paul D. and Soria, Marc and Gaertner, Jean Claude and Robinson, Jan},
