@article{VerHoef2006,
abstract = {We develop spatial statistical models for stream networks that can estimate relationships between a response variable and other covariates, make predictions at unsampled locations, and predict an average or total for a stream or a stream segment. There have been very few attempts to develop valid spatial covariance models that incorporate flow, stream distance, or both. The application of typical spatial autocovariance functions based on Euclidean distance, such as the spherical covariance model, are not valid when using stream distance. In this paper we develop a large class of valid models that incorporate flow and stream distance by using spatial moving averages. These methods integrate a moving average function, or kernel, against a white noise process. By running the moving average function upstream from a location, we develop models that use flow, and by construction they are valid models based on stream distance. We show that with proper weighting, many of the usual spatial models based on Euclidean distance have a counterpart for stream networks. Using sulfate concentrations from an example data set, the Maryland Biological Stream Survey (MBSS), we show that models using flow may be more appropriate than models that only use stream distance. For the MBSS data set, we use restricted maximum likelihood to fit a valid covariance matrix that uses flow and stream distance, and then we use this covariance matrix to estimate fixed effects and make kriging and block kriging predictions.},
author = {{Ver Hoef}, Jay M. and Peterson, Erin and Theobald, David},
doi = {10.1007/s10651-006-0022-8},
file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ver Hoef, Peterson, Theobald - 2006 - Spatial statistical models that use flow and stream distance.pdf:pdf},
isbn = {1352-8505},
issn = {13528505},
journal = {Environmental and Ecological Statistics},
keywords = {Block kriging,Geostatistics,Stream networks,Valid autocovariances,Variogram},
number = {4},
pages = {449--464},
title = {{Spatial statistical models that use flow and stream distance}},
volume = {13},
year = {2006}
}
@article{Sadio2009,
abstract = {This paper discusses comparative advantages of the Slash and Burn System (SBS) practiced by the small farmers in the humid tropics of Central Africa with regard to agricultural mod ernization. The purpose of the paper is to (i) serve to further the dialogue between various stakeholders (communities, practitioners, scientists, policy makers, donors, etc.) in the quest to improve small farmer livelihood and, (ii) confront conflicting attitudes among stakeholders between Slash and Burn practices, sustainable agricultural development and environmental protection. Over centuries, small farmers in humid dense tropical forests of Central Africa, as well as in other parts of Africa, have developed tree-based farming systems based on shifting cultivation and slash-andburn, to cope with low soil fertility and food production decline. The practices consisted to clear forest lands and to grow subsistence crops (rainfed rice, plantain, cassava, maize, etc.) on the fresh ashes from the burned biomass, and putting the exhausted land on fallow for 10 to 15 years after 2 -3 years of production Under normal circumstances, the system is ecologically sustainable with high potential to restore soil fertility and to allow forest recovery. However, by mid-seventies, with the climate degradation and the population fast growth and increasing needs for forest products and fertile agricultural lands, the system started breaking down leading to important environmental degradation. Although they are aware of its negative impacts on the environment and biodiversity, the small farmers living in the Congo Basin forests and savannah continue perpetuating this system, favouring it as the most profitable system with regard to their capacities and low investment conditions. On-going debate is far to reunite stakeholders over the impacts of such practices on environmental degradation and sustainable forestry development. However, whatever is the vision or perception one may have, the fact is land and forest resources, with along biodiversity are at continuous threat. Cost-effective solutions are of great and urgent need to ensure environment sustainability and profitability while securing improved livelihood and reducing poverty. An expanded holistic approach of tree-based farming system that fosters livelihoods and is less risky economically, socially and environmentally should be defined beyond the restrictive concept of agroforestry centred on forest products and environmental benefits},
author = {Sadio, Syaka},
journal = {Forestry},
keywords = {agroforestry,alternatives to slash-and-burn,as well as those,congo basin,congo basin sub-region,equatorial and tropical dense,forests and humid savannahs,have developed,in other forest and,land erosion,negotiating trade-offs,of the,over centuries,slash-and-burn,small farmers of the,woodland areas of africa},
number = {October},
pages = {18--23},
title = {{Alternatives to slash and burn : Negociating trade-offs}},
year = {2009}
}
@article{Lambers2008,
abstract = {Nitrogen (N) tends to limit plant productivity on young soils; phosphorus (P) becomes increasingly limiting in ancient soils because it gradually disappears through leaching and erosion. Plant traits that are regarded as adaptations to N- and P-limited conditions include mycorrhizas and cluster roots. Mycorrhizas 'scavenge' P from solution or 'mine' insoluble organic N. Cluster roots function in severely P-impoverished landscapes, 'mining' P fixed as insoluble inorganic phosphates. The 'scavenging' and 'mining' strategies of mycorrhizal species without and non-mycorrhizal species with cluster roots, respectively, allow functioning on soils that differ markedly in P availability. Based on recent advances in our understanding of these contrasting strategies of nutrient acquisition, we provide an explanation for the distribution of mycorrhizal species on less P-impoverished soils, and for why, globally, cluster-bearing species dominate on severely P-impoverished, ancient soils, where P sensitivity is relatively common. {\textcopyright} 2007 Elsevier Ltd. All rights reserved.},
author = {Lambers, Hans and Raven, John a. and Shaver, Gaius R. and Smith, Sally E.},
doi = {10.1016/j.tree.2007.10.008},
isbn = {0169-5347},
issn = {01695347},
journal = {Trends in Ecology and Evolution},
number = {2},
pages = {95--103},
pmid = {18191280},
title = {{Plant nutrient-acquisition strategies change with soil age}},
volume = {23},
year = {2008}
}
@article{Girardin2013b,
abstract = {Background: The slopes of the eastern Andes harbour some of the highest biodiversity on Earth and a high proportion of endemic species. However, there have been only a few and limited descriptions of carbon budgets in tropical montane forest regions. Aims: We present the first comprehensive data on the production, allocation and cycling of carbon for two high elevation (ca. 3000 m) tropical montane cloud forest plots in the Kos{\~{n}}ipata Valley, Peruvian Andes. Methods: We measured the main components and seasonal variation of net primary productivity (NPP), autotrophic (Ra) and heterotrophic (Rh) respiration to estimate gross primary productivity (GPP) and carbon use efficiency (CUE) in two 1-ha plots. Results: NPP for the two plots was estimated to be 7.05 ± 0.39 and 8.04 ± 0.47 Mg C ha?1 year?1, GPP to be 22.33 ± 2.23 and 26.82 ± 2.97 Mg C ha?1 year?1 and CUE was 0.32 ± 0.04 and 0.30 ± 0.04. Conclusions: We found strong seasonality in NPP and moderate seasonality of Ra, suggesting that forest NPP is driven by changes in photosynthesis and highlighting the importance of variation in solar radiation. Our findings imply that trees invest more in biomass production in the cooler season with lower solar radiation and more in maintenance during the warmer and high solar radiation period.},
author = {Girardin, C{\'{e}}cile a J and Espejob, Javier E Silva and Doughty, Christopher E and Huasco, Walter Huaraca and Metcalfe, Dan B and Durand-Baca, Liliana and Marthews, Toby R and Aragao, Luiz E O C and Farfan-Rios, William and Garc{\'{i}}a-Cabrera, Karina and Halladay, Katherine and Fisher, Joshua B and Galiano-Cabrera, Darcy F and Huaraca-Quispe, Lidia P and Alzamora-Taype, Ivonne and Eguiluz-Mora, Luzmila and Salinas-Revilla, Norma and Silman, Miles R and Meir, Patrick and Malhi, Yadvinder},
doi = {10.1080/17550874.2013.820222},
isbn = {1755-0874},
issn = {1755-0874},
journal = {Plant Ecology {\&} Diversity},
keywords = {Andes,carbon use efficiency,ecophysiology,elevation gradient,net primary productivity,respiration,solar radiation,tropical montane forest},
