file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/van de Weg et al. - 2014 - Gross Primary Productivity of a High Elevation Tropical Montane Cloud Forest.pdf:pdf},
issn = {14350629},
journal = {Ecosystems},
keywords = {Andes,Peru,SPA model,autotrophic respiration,carbon expenditure,carbon fluxes,diurnal photosynthesis,gross primary productivity (GPP),net primary productivity (NPP),sap flow},
number = {5},
