file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kitayama, Aiba - 2002 - Ecosystem structure and productivity of tropical rain forests along altitudinal gradients with contrasting soil.pdf:pdf},
isbn = {0022-0477},
issn = {1365-2745},
journal = {Journal of Ecology},
number = {1},
pages = {37--51},
title = {{Ecosystem structure and productivity of tropical rain forests along altitudinal gradients with contrasting soil phosphorus pools on Mount Kinabalu, Borneo}},
volume = {90},
year = {2002}
}
