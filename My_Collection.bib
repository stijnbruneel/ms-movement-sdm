file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Clark et al. - 2001 - Measuring net primary production in forests Concepts and field methods.pdf:pdf},
isbn = {1051-0761},
issn = {10510761},
journal = {Ecological Applications},
keywords = {Biomass increment,Boreal,Carbon,Coarse roots,Fine root turnover,Forest inventory plots,Litterfall,Net ecosystem C exchange,Net primary production,Temperate,Total belowground carbon allocation,Tropical forests},
