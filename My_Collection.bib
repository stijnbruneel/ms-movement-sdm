doi = {10.1029/2009JF001556},
file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rennie, Church - 2010 - Mapping spatial distributions and uncertainty of water and sediment flux in a large gravel bed river reach using.pdf:pdf},
isbn = {0148-0227},
