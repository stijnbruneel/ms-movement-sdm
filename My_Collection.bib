keywords = {Basal area change,Bolivia,Climate,Disturbance,Logging,Plant-climate interactions,Tree growth,Tropical forest},
number = {5},
pages = {1065--1068},
title = {{Is climate a stronger driver of tree growth than disturbance? A comment on Toledo et al. (2011)}},
volume = {100},
year = {2012}
}
