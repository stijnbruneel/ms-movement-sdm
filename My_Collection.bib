doi = {10.1111/j.1469-8137.2009.02917.x},
file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Craine et al. - 2009 - Global patterns of foliar nitrogen isotopes and their relationships with climate, mycorrhizal fungi, foliar nutri.pdf:pdf},
