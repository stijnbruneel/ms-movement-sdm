file = {:C$\backslash$:/Users/spbrunee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hoffman et al. - 2012 - An advanced data assimilation system for the chesapeake bay Performance evaluation.pdf:pdf},
issn = {07390572},
journal = {Journal of Atmospheric and Oceanic Technology},
keywords = {Data assimilation,Estuaries,Forcing,Ocean models},
number = {10},
pages = {1542--1557},
title = {{An advanced data assimilation system for the chesapeake bay: Performance evaluation}},
volume = {29},
