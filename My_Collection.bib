keywords = {15N natural abundance,Ecological restoration,N mineralization,N/P Economy Index,Nitrogen dynamics,Nutrients cycling},
number = {8},
pages = {1551--1557},
title = {{Nitrogen dynamics during ecosystem development in tropical forest restoration}},
