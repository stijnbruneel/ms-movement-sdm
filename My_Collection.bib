issn = {16821750},
journal = {International Archives of the Photogrammetry, Remote Sensing and Spatial Information Sciences - ISPRS Archives},
keywords = {Fog ecosystems,Geo-ecological modelling,Spatio-spectral classification,Worldview-3},
number = {July},
pages = {251--256},
title = {{Remote sensing-based detection and spatial pattern analysis for geo-ecological niche modeling of tillandsia SPP. In the atacama, Chile}},
