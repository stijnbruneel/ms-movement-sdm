@misc{VanGinneken2005,
abstract = {The European eel (Anguilla anguilla Linnaeus 1758) is a species typical$\backslash$nfor waters of Western Europe. Thanks to early expeditions on the$\backslash$nAtlantic Ocean by the Danish biologist Johannes Schmidt who found$\backslash$nsmall ({\textless} 10mm) leptocephali larvae in the Sargasso Sea about 100$\backslash$nyears ago, we have now a strong indication where the spawning site$\backslash$nfor this species is located. The American eel (Anguilla rostrata,$\backslash$nLeSueur) also spawns in the Sargasso Sea. The spawning time and location$\backslash$nof both species have been supported and refined in recent analyses$\backslash$nof the available historical data. Subsequent ichthyoplankton surveys$\backslash$nconducted by McCleave (USA) and Tesch (Germany) in the 1980s indicated$\backslash$nan increase in the number of leptocephali {\textless} 10 mm , confirming and$\backslash$nrefining the Sargasso Sea theory of Johannes Schmidt. Distinctions$\backslash$nbetween the European and American eel are based on morphological$\backslash$ncharacteristics (number of vertebrae) as well as molecular markers$\backslash$n(allozymes, mitochondrial DNA and anonymous genomic-DNA. Although$\backslash$nrecognised as two distinct species, it remains unclear which mechanisms$\backslash$nplay a role in species separation during larval drift, and what orientation$\backslash$nmechanism eels use during migration in the open sea. The current$\backslash$nstatus of knowledge on these issues will be presented. The hypothesis$\backslash$nthat all European eel migrate to the Sargasso Sea for reproduction$\backslash$nand comprise a single randomly mating population, the so called panmixia$\backslash$ntheory, was until recently broadly accepted. However, based on field$\backslash$nobservations, morphological parameters and molecular studies there$\backslash$nare some indications that Schmidt's claim of complete homogeneity$\backslash$nof the European eel population and a unique spawning location may$\backslash$nbe an overstatement. Recent molecular work on European eel indicated$\backslash$na genetic mosaic consisting of several isolated groups, leading to$\backslash$na rejection of the panmixia theory. Nevertheless, the latest extensive$\backslash$ngenetic survey indicated that the geographical component of genetic$\backslash$nstructure lacked temporal stability, emphasising the need for temporal$\backslash$nreplication in the study of highly vagile marine species. Induced$\backslash$nspawning of hormone treated eels in the aquarium was collective and$\backslash$nsimultaneous. In this work for the first time group spawning behaviour$\backslash$nhas ever been observed and recorded in eels. Studies in swim-tunnels$\backslash$nindicate that eels can swim four to six times more efficiently than$\backslash$nnon-anguilliform fish such as trout. After a laboratory swim trial$\backslash$nof eels over 5,500 km, the body composition did not change and fat,$\backslash$nprotein and carbohydrate were used in the same proportion. This study$\backslash$ndemonstrated for the first time that European eel are physiologically$\backslash$nable of reaching the Sargasso Sea without feeding. Based on catches$\backslash$nof newly hatched larvae, temperature preference tests and telemetry$\backslash$ntracking of mature hormone treated animals, it can be hypothesised$\backslash$nthat spawning in the Sargasso Sea is collective and simultaneous,$\backslash$nwhile presumably taking place in the upper 200 m of the ocean. Successful$\backslash$nsatellite tracking of longfin female eels in New Zealand has been$\backslash$nperformed to monitor migration pathways. Implementation of this new$\backslash$ntechnology is possible in this species because it is three times$\backslash$nlarger than the European eel. In the future, miniaturisation of tagging$\backslash$ntechnology may allow European eels to be tracked in time by satellite.$\backslash$nThe most interesting potential contribution of telemetry tracking$\backslash$nof silver eels is additional knowledge about migration routes, rates,$\backslash$nand depths. In combination with catches of larvae in the Sargasso$\backslash$nSea, it may elucidate the precise spawninglocations of different$\backslash$neel species or groups. Only then, we will be able to define sustainable$\backslash$nmanagement issues by integrating this novel knowledge into spawners$\backslash$nescapement and juvenile fishing quota. },
author = {van Ginneken, V. J T and Maes, Gregory E.},
booktitle = {Reviews in Fish Biology and Fisheries},
doi = {10.1007/s11160-006-0005-8},
isbn = {0960-3166},
issn = {09603166},
keywords = {Anguilla,Migration,Molecular studies,Sargasso Sea,Satellite,Spawning behaviour},
number = {4},
pages = {367--398},
title = {{The European eel (Anguilla anguilla, Linnaeus), its lifecycle, evolution and reproduction: A literature review}},
volume = {15},
year = {2005}
}
@article{Lawton2001,
abstract = {Tropical montane cloud forests (TMCFs) depend on predictable, frequent, and prolonged immersion in cloud. Clearing upwind lowland forest alters surface energy budgets in ways that influence dry season cloud fields and thus the TMCF environment. Landsat and Geostationary Operational Environmental Satellite imagery show that deforested areas of Costa Rica's Caribbean lowlands remain relatively cloud-free when forested regions have well-developed dry season cumulus cloud fields. Further, regional atmospheric simulations show that cloud base heights are higher over pasture than over tropical forest areas under reasonable dry season conditions. These results suggest that land use in tropical lowlands has serious impacts on ecosystems in adjacent mountains.},
author = {Lawton, R O and Nair, U S and {Pielke Sr}, R a and Welch, R M},
doi = {10.1126/science.1062459},
isbn = {0036-8075},
issn = {0036-8075},
