title = {{Salinity and temperature structure of a freezing Arctic fjord-monitored by white whales ( {\textless}i{\textgreater}Delphinapterus leucas{\textless}/i{\textgreater} )}},
url = {http://doi.wiley.com/10.1029/2002GL015462},
